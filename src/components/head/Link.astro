---
import '@/assets/styles/tailwind.css';
import iPhoneFavicon from '@/assets/favicons/iPhone.png';
import browserFavicon from '@/assets/favicons/browser.png';
import androidFavicon from '@/assets/favicons/Android.png';
---

<link
  rel="apple-touch-icon"
  sizes="180x180"
  href={iPhoneFavicon.src}
/>
<link
  rel="icon"
  type="image/png"
  sizes="196x196"
  href={androidFavicon.src}
/>
<link
  rel="icon"
  type="image/png"
  sizes="32x32"
  href={browserFavicon.src}
/>
<link
  rel="icon"
  type="image/png"
  sizes="16x16"
  href={browserFavicon.src}
/>
<link
  rel="mask-icon"
  href="/images/favicons/safari-pinned-tab.svg"
  color="#ffffff"
/>
<!--Ionicons -->
<script
  type="module"
  src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.esm.js"
></script>
<!--
https://rsms.me/inter/
-->
<link rel="preconnect" href="https://rsms.me/" />
<link rel="stylesheet" href="https://rsms.me/inter/inter.css" />
<!---- Alpine  -->
<script defer src="https://unpkg.com/alpinejs@3.10.5/dist/cdn.min.js"></script>
